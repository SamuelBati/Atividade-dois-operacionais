#include<stdio.h>
main(){

float mercadoria;
float frete;
float despesa;
float venda;
float lucro;
float porcentagem;

printf("digite o valor de custo da mercadoria");
scanf("%2f", &mercadoria);

printf("digite o valor do frete");
scanf("%2f", &frete);

printf("digite as despesas");
scanf("%2f", &despesa);

prinf("digite o preco de venda");
scanf("%2f", &venda);

lucro= venda-(mercadoria+frete+despesa);

porcentagem= (lucro/mercadoria+frete+despesa)*100;

printf("A porcentagem de lucro Ã© igual a: %.2f%%", porcentagem);
}

